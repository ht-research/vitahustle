    "c9b1908d-b463-4c61-b3a5-3edeef574d5c": {
      "type": "Pdp-faq",
      "blocks": {
        "cb0524f6-907d-4c41-a008-5e88e87ce48c": {
          "type": "pdp_faq",
          "settings": {
            "question": "What is VitaHustle ONE?",
            "answer": "VitaHustle ONE is a healthy, all-in-one superdrink that contains 86 superfoods and essential nutrients, vegan protein, probiotics and adaptogens. It's called a superdrink because it's power packed blend of superfood fruits and veggies, protein, probiotics, herbs and adaptogens that provide daily nutrition in one simple drink."
          }
        },
        "2982015c-3137-45c0-b028-8eb7f1577a40": {
          "type": "pdp_faq",
          "settings": {
            "question": "Is there sugar in VitaHustle ONE?",
            "answer": "VitaHustle ONE has no added sugar and zero sugar alcohols. While some superfood powders use organic coconut flower nectar in their fruit blend to hide added sugars, we have avoided that. We use a combination of natural flavors, stevia leaf extract and monk fruit so the ONE is pleasant tasting without being overly sweet."
          }
        },
        "4d7eb0f1-e223-4951-85dc-b02b678e9e87": {
          "type": "pdp_faq",
          "settings": {
            "question": "What vegan protein is used in VitaHustle ONE?",
            "answer": "VitaHustle ONE uses an organic blend of the highest quality vegan proteins including pea, quinoa, and mung bean. This balanced blend of protein has an excellent amino acid profile and delivers 20 grams of protein for sustained energy."
          }
        },
        "7ea84d0f-830d-4bf4-9f5a-c7b08e83af9b": {
          "type": "pdp_faq",
          "settings": {
            "question": "How many servings are in each bag?",
            "answer": "Each bag of VitaHustle ONE contains about 15 servings. Use it at least once a day to feel optimal health benefits. We recommend starting with 2 bags per person which is about a 30-day supply."
          }
        },
        "2c0f90e9-1719-4f7f-be2f-bb5d8f4fed16": {
          "type": "pdp_faq",
          "settings": {
            "question": "How do you use VitaHustle ONE?",
            "answer": "Simply add the recommended scoops to 8-12 ounces of ice-cold water, or to a plant-based milk like soy, coconut, or oat. Shake, or mix in a blender for best results, and drink immediately. Add ice if desired. May be added into any smoothie recipe."
          }
        }
      },
