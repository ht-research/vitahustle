{% comment %} ===>{{ serving_product_compare_price }}, {{ serving_product_price }} {% endcomment %}
{% assign temp_compare_price = 0 %}
{% assign temp_total_price = 0 %}
{% for product in electrolytes_product %}
    {% assign temp_compare_price = product.compare_at_price | plus : temp_compare_price %}
    {% assign temp_total_price = product.price | plus : temp_total_price %}
{% endfor %}
{% comment %} ===>temp_compare_price {{ temp_compare_price }}
===>temp_total_price {{ temp_total_price }}
