<script>
    document.addEventListener("DOMContentLoaded", (event) => {
        $('.testimonial_videos_review_wrapper.mobile_only').slick({
            dots: true,
            infinite: false,
            speed: 300,
            slidesToShow: 1,
            slidesToScroll: 1,
            centermode: true,
            centerPadding: "40px"
        });

        $(".video_section").on('click',function(){
            {% comment %} var dataURL = $(this).attr('data-url');
            console.log('dataURL',dataURL,$(this),$(this)[0].paused,$(this).attr('src'));
            if(!$(this).attr('src')){
                $(this).attr('src',dataURL);
            } {% endcomment %}
            if($(this).find('video')[0].paused){
                $(this).find('svg').addClass('hide');
            }
            else{
                $(this).find('svg').removeClass('hide');
            }
            $(this).find('video')[0].paused ? $(this).find('video').trigger('play') : $(this).find('video').trigger('pause');
        })
          
    });
</script>

