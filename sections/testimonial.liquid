
{%- liquid
    assign options = section.settings
    assign testimonial_title = options.testimonial_title
    assign testimonial_subtitle = options.testimonial_subtitle
    assign testimonial_review_title = options.testimonial_review_title
    assign testimonial_review_subtitle = options.testimonial_review_subtitle
    assign testimonial_review_count = options.testimonial_review_count
    assign btn_text = options.btn_text
    assign btn_url = options.btn_url
    assign blocks = section.blocks

  -%}

<style>
.testimonial_wrapper{
    width: 100%;
    max-width: 1440px;
    display: inline-block;
}
.testimonial_wrapper .inner_testimonial_wrapper{
    padding: 40px;
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_title_review_details{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: flex-start;
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_title_review_details .testimonial_title_subtitle{
    display: flex;
    flex-direction: column;
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_title_review_details .testimonial_title_subtitle h3{
    font-family: "Druk Web";
    font-size: 4.514vw;
    font-weight: 700;
    line-height: 60.78px;
    letter-spacing: 1px;
    max-width: 445px;
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_title_review_details .testimonial_title_subtitle p{
    font-family: 'Lato Regular', sans-serif;
    font-size: 1.389vw;
    font-weight: 700;
    line-height: 24px;
    margin: 20px 0px 40px 0px;
    max-width: 606px;
    text-align: left;
    color: #4D201A;
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_title_review_details .testimonial_review_details{
    display: flex;
    flex-direction: column;
    row-gap: 10px;
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_title_review_details .testimonial_review_details .review_count_details{
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
    column-gap: 15px;
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_title_review_details .testimonial_review_details h4{
    font-family: "Druk Web";
    font-size: 3.472vw;
    font-weight: 700;
    line-height: 60px;
    color: #AFC42F;
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_title_review_details .testimonial_review_details .review_count_details p{
    font-family: "Druk Web";
    font-size: 4.931vw;
    font-weight: 700;
    line-height: 71px;
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_title_review_details .testimonial_review_details .review_count_details .review_star_text{
    display: flex;
    flex-direction: column;
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_title_review_details .testimonial_review_details .review_count_details .review_star_text .review_star{
    display: flex;
    flex-direction: row;
    margin-bottom: 10px;
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_title_review_details .testimonial_review_details .review_count_details .review_star_text p{
    font-family: 'Lato Regular', sans-serif;
    font-size: 1.111vw;
    font-weight: 700;
    line-height: 19.2px;
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_title_review_details .testimonial_review_details .review_count_details .review_star_text .review_star svg{
    width: 24px;
    height: 24px;
    margin-right: 8px;
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_videos_review_wrapper{
    display: flex;
    flex-direction: row;
    column-gap: 40px;
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_videos_review_wrapper .video_review_content{
    width: calc((100% - 120px)/4);
    display: flex;
    flex-direction: column;
    row-gap: 40px;
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_videos_review_wrapper .video_review_content:nth-child(2n+1){
    {% comment %} margin-top: 40px; {% endcomment %}

}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_videos_review_wrapper .video_review_content .video_section{
    width: 21.181vw;
    max-width: 305px;
    height: 35.208vw;
    max-height: 507px;
    position: relative;
    cursor: pointer;
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_videos_review_wrapper .video_review_content .video_section svg{
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_videos_review_wrapper .video_review_content .video_section svg.hide{
    display: none;
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_videos_review_wrapper .video_review_content .video_section video,.testimonial_wrapper .inner_testimonial_wrapper .testimonial_videos_review_wrapper .video_review_content .video_section img{
    width: 100%;
    height: 100%;
    object-fit: cover;
    object-position: center;
    border-radius: 12px;
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_videos_review_wrapper .video_review_content .review_content{
    width: 21.181vw;
    max-width: 305px;
    /* height: 32.778vw; */
    /* max-height: 472px; */
    padding: 20px;
    background-color: rgba(175,196,47,.7);
    border-radius: 12px;
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_videos_review_wrapper .video_review_content .review_content .review_image_text{
    display: flex;
    flex-direction: column;
    align-items: center;
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_videos_review_wrapper .video_review_content .review_content .review_image_text .user_image{
    width: 9.306vw;
    max-width: 134px;
    height: 9.306vw;
    max-height: 134px;
    margin-bottom: 20px;
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_videos_review_wrapper .video_review_content .review_content .review_image_text .user_image img{
    width: 100%;
    height: 100%;
    object-fit: cover;
    object-position: center;
    border-radius: 50%;
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_videos_review_wrapper .video_review_content .review_content .review_image_text .review_text{
    display: block;

}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_videos_review_wrapper .video_review_content .review_content .review_image_text .review_text p{
    font-family: 'Lato Regular', sans-serif;
    font-style: italic;
    font-size: 1.328vw;
    font-weight: 500;
    line-height: 30px;
    text-align: center;
    color: #4D201A;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 6;
    overflow: hidden;
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_videos_review_wrapper .video_review_content .review_content .reviewer_details{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    margin-top: 20px;
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_videos_review_wrapper .video_review_content .review_content .reviewer_details p{
    font-family: 'Lato Regular', sans-serif;
    /* font-style: italic; */
    font-size: 1.111vw;
    font-weight: 600;
    line-height: 25.2px;
    text-align: center;
    color: #212121; 
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_videos_review_wrapper .video_review_content .review_content .reviewer_details .icon svg{
    width: 36px;
    height: 36px;
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_videos_review_wrapper .video_review_content .review_content .reviewer_details span{
    font-family: 'Lato Regular', sans-serif;
    font-style: italic;
    font-size: 0.833vw;
    font-weight: 600;
    line-height: 15px !important;
    text-align: left;
    color: #212121;
    max-width: 66px;
}
.testimonial_wrapper .inner_testimonial_wrapper .all_reviews{
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: flex-start;
    width: 100%;
    margin-top: 40px;
}
.testimonial_wrapper .inner_testimonial_wrapper .all_reviews a{
    background-color: #C8E812;
    font-family: 'Lato Regular', sans-serif;
    /* font-style: italic; */
    font-size: 1.389vw;
    font-weight: 600;
    line-height: 24px;
    /* padding: 10px 40px; */
    border-radius: 74px;
    text-decoration: none;
    width: 18.125vw;
    height: 3.472vw;
    text-align: center;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    height: 60px !important;
    display: inline-flex !important;
    justify-content: center !important;
    align-items: center !important;
    padding-top: 0px !important;
    padding-bottom: 0px !important;
}
.testimonial_wrapper .inner_testimonial_wrapper .testimonial_videos_review_wrapper.mobile_only{
    display: none;
}
@media screen and (min-width: 1441px){
    .testimonial_wrapper{
        display: block;
        {% comment %} margin: 0 auto; {% endcomment %}
        max-width: unset;        
    }
    .testimonial_wrapper .inner_testimonial_wrapper .testimonial_title_review_details .testimonial_title_subtitle h3{
        font-size: 70px;
        max-width: unset;        
    }
    .testimonial_wrapper .inner_testimonial_wrapper .testimonial_title_review_details .testimonial_title_subtitle p{
        line-height: 100%;
    }
