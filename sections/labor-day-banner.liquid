
{%- liquid
    assign options = section.settings
    assign labor_day_banner_title = options.labor_day_banner_title
    assign labor_day_banner_subtitle = options.labor_day_banner_subtitle
    assign labor_day_banner_subtext = options.labor_day_banner_subtext
    assign labor_day_banner_offerText = options.labor_day_banner_offerText
    assign background_larger_screen_image = options.background_larger_screen_image
    assign background_desktop_image = options.background_desktop_image
    assign background_mobile_image = options.background_mobile_image
    assign background_ipad_image = options.background_ipad_image
    assign desktop_banner_max_height = options.desktop_banner_max_height
    assign mobile_banner_min_height = options.mobile_banner_min_height
    assign mobile_banner_max_height = options.mobile_banner_max_height
    assign Tablet_banner_max_height = options.Tablet_banner_max_height
    assign larger_screen_banner_max_height = options.larger_screen_banner_max_height
    assign btn_text = options.btn_text
    assign btn_url = options.btn_url
    assign blocks = section.blocks

  -%}

<style>
    .labor_day_banner{
        width: 100%;
        height: 50.181vw;
        {% if desktop_banner_max_height !=blank %}
            max-height: {{ desktop_banner_max_height }}px;
        {% endif %}
        display: block;
        background-image: url("{{ background_desktop_image | img_url: 'master' }}");
        background-repeat: no-repeat;
        background-size: cover;
        background-position: center;
    }
    .labor_day_banner .inner_labor_day_banner{
        padding: 40px 40px 20px 40px;
        display: flex;
        flex-direction: column;
        justify-content: space-between;
        height: 100%;
        position: relative;
    }
    .labor_day_banner .inner_labor_day_banner .top_content{
        display: flex;
        flex-direction: column;
        align-items: center;
        max-width: 44.644vw;
        position: absolute;
        top: 55px;
        left: 53px;
    }
    .labor_day_banner .inner_labor_day_banner .top_content p{
        font-family: "Druk Web";
        color: #FFF;
        text-shadow: 0px 8px 14px rgba(0, 0, 0, 0.20);
        font-size: 15.582vw;
        font-style: normal;
        font-weight: 700;
        line-height: 70%;
        margin: 9px 0px 22px 0px;
    }
    .labor_day_banner .inner_labor_day_banner .top_content p span{
        color: #C8E812;
        font-size: 8.8832vw;
        line-height: 9.659vw;
    }
    .labor_day_banner .inner_labor_day_banner .top_content .subtitle{
        font-family: "Druk Web";
        text-shadow: 0px 5px 10px rgba(0, 0, 0, 0.20);
        font-size: 8.611vw;
        font-style: normal;
        font-weight: 700;
        line-height: 88%;
        color: #C8E812;
        {% comment %} margin: 10px 0px 20px 0px; {% endcomment %}
    }
    .labor_day_banner .inner_labor_day_banner .top_content h4{
        font-family: "TestSohneBreit";
        color: #FFF;
        text-shadow: 0px 5.722px 17.166px rgba(0, 0, 0, 0.14);
        font-size: 1.667vw;
        font-style: normal;
        font-weight: 500;
        line-height: normal;
        text-transform: uppercase;
        margin: 34px 0px;
    }
    .labor_day_banner .inner_labor_day_banner .top_content a{
        background: #C8E812;
        border-radius: 74px;
        display: inline-flex;
        align-items: center;
        justify-content: center;
        width: 233px;
        height: 55px;
        font-family: "FK Grotesk Bold", sans-serif;
        color: #4D201A;
        text-align: center;
        font-size: 22px;
        font-style: normal;
        font-weight: 700;
        line-height: normal;
        text-transform: uppercase;
        text-decoration: none;
    }
    .labor_day_banner .inner_labor_day_banner .bottom_subtext{
        color: #000;
        font-family: "Lato Regular", sans-serif;
        font-size: 14px;
        font-style: normal;
        font-weight: 900;
        line-height: normal;
        position: absolute;
        bottom: 15px;
        left: 10px;
    }
    .labor_day_banner .inner_labor_day_banner .bottom_content{
        display: flex;
        flex-direction: column;
        align-items: flex-end;
    }
    .labor_day_banner .inner_labor_day_banner .bottom_content .feature_icon_wrapper{
        display: flex;
        flex-direction: row;
        column-gap: 20px;
    }
    .labor_day_banner .inner_labor_day_banner .bottom_content .feature_icon_wrapper .feature_icon{
        width: 76px;
        height: 76px;
        display: block;
    }
    .labor_day_banner .inner_labor_day_banner .bottom_content .feature_icon_wrapper .feature_icon img{
        width: 100%;
        height: 100%;
        object-fit: cover;
        object-position: center;
        display: block;
    }
    .labor_day_banner .inner_labor_day_banner .bottom_content .shop_btn{
        display: none;
    }
    .labor_day_banner .inner_labor_day_banner .bottom_content .shop_btn a{
        border-radius: 74px;
        background-color: #C8E812;
        display: inline-flex;
        align-items: center;
        justify-content: center;
        width: 261px;
        height: 55px;
        font-family: 'Lato Bold', sans-serif;
        font-size: 1.388vw;
        font-weight: 900;
        line-height: 1.665vw;
        text-decoration: none;
    }
    .bottom_arrow_icon{
