                <p>{{ labor_day_banner_title }}</p>
            {% endif %}
            {% if labor_day_banner_subtext != blank %}
                <h4>{{ labor_day_banner_subtext }}</h4>
            {% endif %}
            
            {% if btn_text != blank %}
                <a href="{{ btn_url }}">{{ btn_text }}</a>
            {% endif %}
        </div>
        <div class="bottom_subtext">
            {% if labor_day_banner_offerText !=blank %}
                {{ labor_day_banner_offerText }}
            {% endif %}
            
        </div>
    </div>
</div>

{% schema %}
  {
    "name": "Labor Day Banner",
    "tag": "section",
    "settings": [

        {
            "type": "text",
            "id": "labor_day_banner_title",
            "label": "t:sections.labor_day_banner.labor_day_banner_title"
        },
        {
            "type": "text",
            "id": "labor_day_banner_subtitle",
            "label": "t:sections.labor_day_banner.labor_day_banner_subtitle"
        },
        {
            "type": "text",
            "id": "labor_day_banner_subtext",
            "label": "t:sections.labor_day_banner.labor_day_banner_subtext"
        },
        {
            "type": "text",
            "id": "labor_day_banner_offerText",
            "label": "t:sections.labor_day_banner.labor_day_banner_offerText"
        },
        {
            "type": "image_picker",
            "id": "background_larger_screen_image",
            "label": "t:sections.labor_day_banner.background_larger_screen_image"
          },
        {
            "type":"range",
            "id":"larger_screen_banner_max_height",
            "label":"t:sections.labor_day_banner.larger_screen_banner_max_height",
            "min": 0,
            "max": 1240,
            "step": 20,
            "default":700
        },
        {
          "type": "image_picker",
          "id": "background_desktop_image",
          "label": "t:sections.labor_day_banner.background_desktop_image"
        },
        {
            "type":"range",
            "id":"desktop_banner_max_height",
            "label":"t:sections.labor_day_banner.desktop_banner_max_height",
            "min": 0,
            "max": 1240,
            "step": 20,
            "default":600
        },
        {
            "type": "image_picker",
            "id": "background_mobile_image",
            "label": "t:sections.labor_day_banner.background_mobile_image"
        },
        {
            "type":"range",
            "id":"mobile_banner_min_height",
            "label":"t:sections.labor_day_banner.mobile_banner_min_height",
            "min": 0,
            "max": 1240,
            "step": 20,
            "default":660
        },
        {
            "type":"range",
            "id":"mobile_banner_max_height",
            "label":"t:sections.labor_day_banner.mobile_banner_max_height",
            "min": 0,
            "max": 1240,
            "step": 20,
            "default":800
        },
        {
            "type": "image_picker",
            "id": "background_ipad_image",
            "label": "t:sections.labor_day_banner.background_ipad_image"
        },
        {
            "type":"range",
            "id":"Tablet_banner_max_height",
            "label":"t:sections.labor_day_banner.Tablet_banner_max_height",
            "min": 0,
            "max": 1240,
            "step": 20,
            "default":800
        },
        {
            "type": "text",
            "id":"btn_text",
            "label": "t:sections.labor_day_banner.btn_text"
        },
        {
            "type": "url",
            "id": "btn_url",
            "label": "t:sections.labor_day_banner.btn_url"
        }
    ],
    "presets": [
        {
        "name": "Labor Day Banner"
        }
    ]
}
{% endschema %}