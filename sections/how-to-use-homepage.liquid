{% render 'inline-css-minifier', css: css %}

<div class="section-{{ section.id }} {% if enable_background_around_container %} section color-{{ color_scheme }} {% endif %}">
  <div id="section_spacing_container" class=" section-spacing container container--{{ container_size }} image-banner__outer-container {% if container_size == 'full-screen' %} image-banner__outer-container--full-screen{% endif %}">
    <div class="how_to_use_content mobile_only extra_gap">
      <h2>{{ section.settings.Heading }}</h2>
    </div>
    <div id="image-banner" class="image-banner {% unless enable_background_around_container %} section color-{{ color_scheme }} {% endunless %}">
      <div class="image-banner__container">
        {%- if btn_text == blank and extra_btn_text == blank -%}
          {%- if btn_url.url != blank or extra_btn_url.url != blank -%}
            <a
              class="image-banner__link"
              href="{% if btn_url.url != blank %}{{ btn_url.url }}{% else %}{{ extra_btn_url.url }}{% endif %}"
              aria-label="{% if btn_text != blank %}{{ btn_text }}{% elsif extra_btn_text != blank %}{{ extra_btn_text }}{% elsif btn_url.title != blank %}{{ btn_url.title }}{% elsif extra_btn_url.title != blank %}{{ extra_btn_url.title }}{% else %}#{% endif %}"></a>
          {%- endif -%}
        {%- endif -%}

        {% if desktop_img != blank %}
          <div class="desktop_video_wrapper">
            <img
              loading="eager"
              alt="desktop_banner"
              class="desktop_only"
              src="{{ desktop_img | img_url: '1440x' }}"
              srcset="{{ desktop_img | img_url: 'medium' }} 600w, {{ desktop_img | img_url: '1600x' }} 1400w" />
          </div>
        {% elsif video != blank %}
          <div class="desktop_video_wrapper">
            {{ video | video_tag: image_size: '1440x', autoplay: true, loop: true, muted: true, controls: false, poster: poster }}
          </div>
        {% endif %}

        {% if mobile_img != blank %}
          <div class="mobile_video_wrapper">
            <img
              loading="eager"
              alt="mobile_banner"
              class="mobile_only"
              src="{{ mobile_img | img_url: '800x' }}"
              srcset="{{ mobile_img | img_url: '700x' }} 600w, {{ mobile_img | img_url: '1600x' }} 1400w" />
          </div>
        {% elsif mobile_video != blank %}
          <div class="mobile_video_wrapper">

            {{ mobile_video | video_tag: image_size: '768x', autoplay: true, loop: true, muted: true, controls: false, poster: mposter }}
          </div>
        {% endif %}

        {% comment %} <div class="bottom_arrow_icon">
          <svg
            xmlns="http://www.w3.org/2000/svg"
            width="35"
            height="24"
            viewBox="0 0 35 24"
            fill="none">
            <path
              d="M3.5 5L17 18.5L31.5 4"
              stroke="white"
              stroke-width="7"
              stroke-linecap="round" />
          </svg>
        </div> {% endcomment %}


        {%- render 'image-banner-content'
          , heading_size: heading_size,
          body_size: body_size,
          title: title,
          caption: caption,
          text: text,
          btn_text: btn_text,
          btn_url: btn_url,
          extra_btn_text: extra_btn_text,
          extra_btn_url: extra_btn_url,
          btn_style: btn_style,
          extra_btn_style: extra_btn_style -%}

      </div>
    </div>
    <div class="how_to_use_content">
      <h2>{{ section.settings.Heading }}</h2>
      <div class="how_to_use_steps">
        {% for block in blocks %}
          {% if block.type=="text" %}
            {% assign index = forloop.index0 | plus : 1 %}
            <div class="steps">
              <div class="step_count_title">
                <div class="step_count">
                  {{ index }}
                </div>
                <h4>{{ block.settings.how_to_use_step_title }}</h4>
              </div>
              {% comment %} <p>cool</p> {% endcomment %}
              {% if block.settings.how_to_use_step_content !=blank %}
                 <p>{{ block.settings.how_to_use_step_content }}</p>
                 {% comment %} <p class="how_to_use_step_content_para t">{{ block.settings.how_to_use_step_content }}</p> {% endcomment %}
              {% endif %}
              
            </div>
          {% endif %}
        {% endfor %}

      </div>
    </div>
  </div>
</div>

