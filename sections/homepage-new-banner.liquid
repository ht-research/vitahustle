{%- liquid
    assign options = section.settings
  
    assign desktop_img = options.desktop_img
    assign mobile_img = options.mobile_img
    assign poster = options.video_poster
    assign mposter = options.m_video_poster
  
    assign video = options.video
    assign mobile_video = options.mobile_video
  
    assign heading_size = options.heading_size
    assign body_size = options.body_size
  
    assign enable_background_around_container = options.enable_background_around_container
  
    assign title = options.title
    assign caption = options.caption
    assign text = options.text
  
    assign btn_text = options.btn_text
    assign btn_url = options.btn_url
    assign btn_style = options.btn_style
  
    assign extra_btn_text = options.extra_btn_text
    assign extra_btn_url = options.extra_btn_url
    assign extra_btn_style = options.extra_btn_style
  
    assign color_scheme = options.color_scheme
    assign mobile_height = options.mobile_height
    assign desktop_height = options.desktop_height
    assign section_spacing_top = options.section_spacing_top
    assign section_spacing_bottom = options.section_spacing_bottom
    assign container_size = options.container_size
  
    assign mobile_content_position = options.mobile_content_position
    assign desktop_content_position = options.desktop_content_position
  
    assign desktop_content_align = options.desktop_content_align
    assign mobile_content_align = options.mobile_content_align
  
    assign mobile_content_background = options.mobile_content_background
    assign desktop_content_background = options.desktop_content_background
  
    assign mobile_overlay_opacity = options.mobile_overlay_opacity | times: 0.01
    assign desktop_overlay_opacity = options.desktop_overlay_opacity | times: 0.01
  
    assign content_max_width = options.content_max_width | divided_by: 10.0
    assign content_spacing = options.content_spacing | divided_by: 10.0
  
    assign has_content = false
  
    if caption != blank or title != blank or text != blank or btn_text != blank or extra_btn_text != blank
      assign has_content = true
    endif
  -%}
  
  {% capture css %}
    {% style %}
  
    
      
      .mobile_video_wrapper {
        display: none !important;
        width: 100%;
      }
      .desktop_video_wrapper {
        display: block !important;
        width: 100%;
      }
      .desktop_video_wrapper > img,
      .mobile_video_wrapper > img {
        width: 100%;
        height: 100%;
        /* object-fit: cover; */
        object-fit: contain;
        object-position: center;
        display: block;
      }
      .section-{{ section.id}}{
        --gsc-height: {{ mobile_height}}
        px;
        --gsc-overlay-opacity: {{ mobile_overlay_opacity}}
        ;
        --gsc-content-width: {{ content_max_width }}
        rem;
        --gsc-content-spacing: {% if has_content %}
          {{ content_spacing }}
          rem{% else %}
          0rem{% endif %};
        --gsc-content-align: {{ mobile_content_align }}
        ;
        --gsc-section-spacing-top: {{ section_spacing_top }}
        px;
        --gsc-section-spacing-bottom: {{ section_spacing_bottom }}
        px;
      }
  
      section.image_banner_new_section .image-banner__container {
        padding: 0;
      }
      section.image_banner_new_section .image-banner__content-wrap {
        position: absolute;
        top: 46%;
        left: 40px;
        transform: translateY(-45%);
      }
      section.image_banner_new_section .image-banner__container video {
        /* position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%); */
        min-width: 100%;
        min-height: 100%;
        width: auto;
        height: auto;
        object-fit: cover;
      }
      section.image_banner_new_section .image-banner {
        /* height: 90vh; */
        /* min-height: 600px; */
        overflow: hidden;
      }
  
      @media screen and (min-width: 768px) {
        section.image_banner_new_section .image-banner__content-wrap {
            justify-content: flex-start;
        }
        .image-banner__btns{
            justify-content: flex-start;
        }
        .image-banner__container{
            min-height: unset;
        }
        section.image_banner_new_section .image-banner__content-wrap{
            justify-content: flex-start;
        }
        
      }
  
      .section-{{ section.id }}.image-banner__content-wrap {
        {{ mobile_content_position }}
  
      }
  
      @media{% render 'media-queries', screen: 'md' %}{
        .section-{{ section.id }}.image-banner__content-wrap {
          {{ desktop_content_position }}
        }
  
        .section-{{ section.id }}{
          --gsc-height: {{ desktop_height }}
          px;
          --gsc-overlay-opacity: {{ desktop_overlay_opacity }}
          ;
          --gsc-content-align: {{ desktop_content_align }}
          ;
        }
      }
      .image_banner_new_section h2 {
        font-size: 6.8vw;
        text-shadow: 0px 4px 8px rgba(0, 0, 0, 0.20);
        font-style: normal;
        font-weight: 700;
        line-height: 8vw;
        text-transform: uppercase;
        text-align: left;
      }
      .image_banner_new_section h2 span {
        color: #C8E812;
      }
      .image_banner_new_section .image-banner__content {
        max-width: 62.94vw;
        padding: 4.8rem 0;
      }
      .image_banner_new_section .image-banner__content .body3 p {
        text-shadow: 0 4px 4px rgba(0, 0, 0, 0.25);
        font-size: 2vw;
        line-height: 120%;
        font-weight: 700;
        font-family: 'Lato Heavy', sans-serif;
      }
  
  
      @media screen and (max-width: 768px) {
        .section-{{ section.id }}.media__image--desktop {
          display: block !important;
        }
        .mobile_video_wrapper {
          display: block !important;
        }
        .desktop_video_wrapper {
          display: none !important;
        }
        .image_banner_new_section .image-banner__content {
          max-width: 100%;
          padding: 70px 0 0;
        }
        section.image_banner_new_section .image-banner {
          /* max-height: 480px;
          min-height: 656px; */
          max-height: unset;
          min-height: unset;
        }
  
        .image_banner_new_section .image-banner__content h2 {
            color: #FFF;
            text-align: center;
            text-shadow: 0px 4px 8px rgba(0, 0, 0, 0.21);
            font-size: 62px;
            font-style: normal;
            font-weight: 700;
            line-height: 64px;
            text-transform: uppercase;
        }
        .image_banner_new_section .image-banner__content p {
          font-size: 18px !important;
          text-align: center;
          width: calc(100% - 40px);
          margin: 0 auto;
        }
        section.image_banner_new_section .image-banner__content-wrap{
            position: absolute;
            top: 108px;
            left: unset;
        }
        .image-banner__btns .btn--solid{
            padding: 14px 38px;
        }
        .image-banner__container{
            min-height: unset;
        }
        .image-banner__content>*+.image-banner__btns {
            margin: 2rem 0 0;
        }
        .image-banner__content>*+.image-banner__btns{
            justify-content: center;
        }
        section.image_banner_new_section .image-banner__content-wrap{
            justify-content: center;
            top: 188px;
        }
        .image_banner_new_section .image-banner__content h2{
            font-size: 82px;
            font-weight: 700;
            line-height: 84px;
        }
      }
      @media screen and (max-width: 481px) {
        .image_banner_new_section .image-banner__content {
          padding-top: 0;
        }
        .image_banner_new_section .image-banner__content h2{
            font-size: 62px;
            font-style: normal;
            font-weight: 700;
            line-height: 64px;
        }
        section.image_banner_new_section .image-banner__content-wrap {
            justify-content: center;
            top: 108px;
        }
      }
      @media screen and (max-width: 360px) {
        .image_banner_new_section .image-banner__content h2 {
          /* font-size: 90px; */
        }
      }
      .bottom_arrow_icon {
        position: absolute;
        bottom: 1.73vw;
        width: 2.42vw;
        min-width: 34px;
        display: flex;
        justify-content: center;
        align-items: center;
        left: 50%;
        z-index: 3;
        cursor: pointer;
        transform: translateX(-50%);
  
      }
      @media screen and (max-width: 481px) {
        .bottom_arrow_icon {
          bottom: 30px;
        } 
      }
      @media screen and (max-width: 540px) {
        section.image_banner_new_section .image-banner{
          max-height: unset;
          min-height: 176vw;
          height: auto;
          min-height: unset;
        }
        section.image_banner_new_section .image-banner__container{
          /* height: 176vw; */
          height: auto;
          min-height: unset;
        }
        section.image_banner_new_section .image-banner__container video{
          width: 100%;
        }
      }
    {% endstyle %}
  {% endcapture %}
  
  {% render 'inline-css-minifier', css: css %}
  
  <div class="section-{{ section.id }} {% if enable_background_around_container %} section color-{{ color_scheme }} {% endif %}">
    <div class=" section-spacing container container--{{ container_size }} image-banner__outer-container {% if container_size == 'full-screen' %} image-banner__outer-container--full-screen{% endif %}">
      <div class="image-banner {% unless enable_background_around_container %} section color-{{ color_scheme }} {% endunless %}">
        <div class="image-banner__container">
          {%- if btn_text == blank and extra_btn_text == blank -%}
            {%- if btn_url.url != blank or extra_btn_url.url != blank -%}
              <a
                class="image-banner__link"
                href="{% if btn_url.url != blank %}{{ btn_url.url }}{% else %}{{ extra_btn_url.url }}{% endif %}"
                aria-label="{% if btn_text != blank %}{{ btn_text }}{% elsif extra_btn_text != blank %}{{ extra_btn_text }}{% elsif btn_url.title != blank %}{{ btn_url.title }}{% elsif extra_btn_url.title != blank %}{{ extra_btn_url.title }}{% else %}#{% endif %}"></a>
            {%- endif -%}
          {%- endif -%}
  
          {% if desktop_img != blank %}
            <div class="desktop_video_wrapper">
              <img
                loading="eager"
                alt="desktop_banner"
                class="desktop_only"
                src="{{ desktop_img | img_url: '3000x' }}"
                srcset="{{ desktop_img | img_url: 'medium' }} 1000w, {{ desktop_img | img_url: '3000x' }} 3000w" />
            </div>
          {% elsif video != blank %}
            <div class="desktop_video_wrapper">
              {{ video | video_tag: image_size: '1440x', autoplay: true, loop: true, muted: true, controls: false, poster: poster }}
            </div>
          {% endif %}
  
          {% if mobile_img != blank %}
            <div class="mobile_video_wrapper">
              <img
                loading="eager"
                alt="mobile_banner"
                class="mobile_only"
                src="{{ mobile_img | img_url: '2200x' }}"
                srcset="{{ mobile_img | img_url: '2200x' }} 600w, {{ mobile_img | img_url: '2200x' }} 22000w" />
            </div>
          {% elsif mobile_video != blank %}
            <div class="mobile_video_wrapper">
  
              {{ mobile_video | video_tag: image_size: '768x', autoplay: true, loop: true, muted: true, controls: false, poster: mposter }}
            </div>
          {% endif %}
  
          <div class="bottom_arrow_icon">
            <svg
              xmlns="http://www.w3.org/2000/svg"
              width="35"
              height="24"
              viewBox="0 0 35 24"
              fill="none">
              <path
                d="M3.5 5L17 18.5L31.5 4"
                stroke="white"
                stroke-width="7"
                stroke-linecap="round" />
            </svg>
          </div>
  
  
          {%- render 'new-image-banner-content'
            , heading_size: heading_size,
            body_size: body_size,
            title: title,
            caption: caption,
            text: text,
            btn_text: btn_text,
            btn_url: btn_url,
            extra_btn_text: extra_btn_text,
            extra_btn_url: extra_btn_url,
            btn_style: btn_style,
            extra_btn_style: extra_btn_style -%}
  
        </div>
      </div>
    </div>
  </div>
  
  <script>
    document.addEventListener('DOMContentLoaded', () => {
      $('.bottom_arrow_icon').on('click', function() {
        let offset = $('.more_than_protein,.new_all_in_one').offset().top;
        $('body,html').animate({
          scrollTop: offset - 40
        }, 400);
      })
    })
  </script>
  {% if template contains 'sl-35C38440' %}
  <style>
    .section-{{ section.id}} .image-banner__content-wrap {top: 45% !important;height: 100%;}
    .section-{{ section.id}} .image-banner__btns {bottom: 12% !important;position: absolute;}
    .section-{{ section.id}} .image-banner__btn {background: #afc42f !important;}
    @media screen and (min-width: 992px) {
      .section-{{ section.id}} .image-banner__btn {padding: 20px 60px;font-size: 20px;}
    }
    @media screen and (min-width: 1600px) and (max-width: 1919px) {
      .section-{{ section.id}} .image-banner__content-wrap {top: 50% !important;}
      .section-{{ section.id}} .image-banner__btns {bottom: 16% !important;}
    }
    @media screen and (min-width: 1920px) {
      .section-{{ section.id}} .image-banner__content-wrap {top: 45% !important;}
      .section-{{ section.id}} .image-banner__btns {bottom: 12% !important;}
    }
    .section-{{ section.id}} .image-banner__btn:hover {background: #fff !important;}
    @media screen and (max-width: 767px) {
      .section-{{ section.id}} .image-banner__content h2 {font-size: 77px;}
      .section-{{ section.id}} .image-banner__content-wrap {top: 50% !important;}
      .section-{{ section.id}} .image-banner__btns {bottom: 22% !important;}
    }
  </style>
  {% endif %}
