<div class="free_shaker_banner">
    <div class="inner_free_shaker_banner">
        <div class="bottom_arrow_icon">
            <svg
              xmlns="http://www.w3.org/2000/svg"
              width="35"
              height="24"
              viewBox="0 0 35 24"
              fill="none">
              <path
                d="M3.5 5L17 18.5L31.5 4"
                stroke="white"
                stroke-width="7"
                stroke-linecap="round" />
            </svg>
          </div>
        <div class="top_content">
            {% if free_shaker_banner_title != blank %}
                <p>{{ free_shaker_banner_title }}</p>
            {% endif %}
            {% if free_shaker_banner_subtitle != blank %}
                <span class="subtitle">{{ free_shaker_banner_subtitle }}</span>
            {% endif %}
            {% if free_shaker_banner_subtext != blank %}
                <h4>{{ free_shaker_banner_subtext }}</h4>
            {% endif %}
            
            {% if btn_text != blank %}
                <a href="{{ btn_url }}">{{ btn_text }}</a>
            {% endif %}
        </div>
        <div class="bottom_subtext">
            {% if free_shaker_banner_offerText !=blank %}
                {{ free_shaker_banner_offerText }}
            {% endif %}
            
        </div>
        {% comment %} <div class="bottom_content">
            {% comment %} <div class="feature_icon_wrapper">
                {% for block in blocks %}
                    <div class="feature_icon">
                        <img src="{{ block.settings.icon_image | img_url: 'master' }}">
                    </div>

                {% endfor %}
            </div> {% endcomment %}
            <div class="shop_btn">
                {% if btn_text != blank %}
                    <a href="{{ btn_url }}">{{ btn_text }}</a>
                {% endif %}
            </div>
        </div> {% endcomment %}
    </div>
</div>

<script>
    document.addEventListener('DOMContentLoaded', () => {
      $('.bottom_arrow_icon').on('click', function() {
        let offset = $('.new_all_in_one').offset().top;
        $('body,html').animate({
          scrollTop: offset - 40
        }, 400);
      })
    })
  </script>

