
{%- liquid
    assign options = section.settings
    assign founded_by_title = options.founded_by_title
    assign founded_by_subtitle = options.founded_by_subtitle
    assign founder_desktop_image = options.founder_desktop_image
    assign founder_mobile_image = options.founder_mobile_image
    
  -%}
  {% comment %} {{ founded_by_title }}
  {{ founded_by_subtitle }}
  {{ founder_desktop_image }}
  {{ founder_mobile_image }} {% endcomment %}
  
  <style>
    .founded_by{
      width: 100%;
      max-width: 1440px;
      display: inline-block;
    }
    .founded_by .inner_founded_by{
      padding: 80px;
      position: relative;
      display: flex;
      flex-direction: row;
      justify-content: space-between;
      background: linear-gradient(to right, #7A4831 0%, #9C6D56 50%, #7A4831 100%);
    }
    .founded_by .inner_founded_by .title{
      max-width: 325px;
    }
    .founded_by .inner_founded_by .title p{
      font-family: "Druk Web";
      font-size: 7.5vw;
      font-weight: 700;
      line-height: 100.98px;
      letter-spacing: 2px;
      color: #FFFFFF;
    }
    .founded_by .inner_founded_by .title p span{
      color: #AFC42F;
    }
  
    .founded_by .inner_founded_by .kevin_image_desktop{
      position: absolute;
      width: 53.611vw;
      max-width: 772px;
      height: 38.403vw;
      max-height: 695px;
      bottom: 0px;
      left: 250px;
    }
    .founded_by .inner_founded_by .kevin_image_desktop img{
      width: 100%;
      height: 100%;
      object-fit: cover;
      object-position: center;
    }
    .founded_by .inner_founded_by .kevin_image_mobile{
      display: none;
    }
    .founded_by .inner_founded_by .right_content{
      max-width: 390px;
      padding: 20px 20px 20px 30px;
      border-left: 1px solid rgba(255,255,255,.3);
    }
    .founded_by .inner_founded_by .right_content .content_quote{
      width: 49px;
      height: 56px;
    }
    .founded_by .inner_founded_by .right_content p{
      font-family: 'Lato Regular', sans-serif;
      font-size: 1.528vw;
      font-weight: 500;
      line-height: 34px;
      color: #FFFFFF;
      margin-bottom: 40px;
    }

    @media screen and (min-width: 1441px){
      .founded_by{
        display: block;
        {% comment %} margin: 0 auto; {% endcomment %}
        max-width: unset;
        background: linear-gradient(to right, #7A4831 0%, #9C6D56 50%, #7A4831 100%);
      }
      .founded_by .inner_founded_by{
        {% comment %} max-width: 1440px; {% endcomment %}
        max-width: 1440px;
        margin: 0 auto;
        background: unset;
      }
      .founded_by .inner_founded_by .title p{
        font-size: 127px;
        font-weight: 700;
        line-height: 120.98px;
      }
      .founded_by .inner_founded_by .right_content{
        max-width: 450px;
      }
      .founded_by .inner_founded_by .kevin_image_desktop{
        left: 280px;
        height: 37.403vw;
      }
      .founded_by .inner_founded_by{
        padding: 120px 80px;
      }
      .founded_by .inner_founded_by .kevin_image_desktop{
        height: 531px !important;
        width: 649px !important;
        left: 310px !important;
        bottom: -20px;

      }
      .founded_by .inner_founded_by .right_content p{
        font-size: 23px;
      }
    }
    @media screen and (max-width: 1270px) and (min-width: 821px){
      .founded_by .inner_founded_by{
        padding: 80px 60px;
      }
      .founded_by .inner_founded_by .title{
        max-width: 265px;
      }
      .founded_by .inner_founded_by .title p{
        line-height: 90.98px;
      }
      .founded_by .inner_founded_by .kevin_image_desktop{
        width: 47.611vw;
        left: 190px !important;
      }
      .founded_by .inner_founded_by .right_content{
        max-width: 350px;
      }
    }
    @media screen and (max-width: 820px) {
      .founded_by .inner_founded_by .kevin_image_desktop{
        display: none;
      }
      .founded_by .inner_founded_by{
        flex-direction: column;
        padding: 20px;
        overflow: hidden;
      }
      .founded_by .inner_founded_by .right_content p{
        margin-bottom: 20px;
      }
      .founded_by .inner_founded_by .right_content svg{
        width: 25px;
        height: 55px;
      }
      .founded_by .inner_founded_by .kevin_image_mobile{
        display: flex;
        justify-content: flex-end;
      }
      .founded_by .inner_founded_by .kevin_image_mobile{
        width: 325px;
        height: 303px;
        position: absolute;
        top: 14px;
        right: -31px;
        display: block;
      }
      .founded_by .inner_founded_by .kevin_image_mobile img{
        width: 100%;
        height: 100%;
        object-fit: contain;
        object-position: center;
      }
      .founded_by .inner_founded_by .right_content {
        margin-top: 82px;
        border: 1px solid rgba(255, 255, 255, .3);
        position: relative;
        padding: 30px 20px 20px 20px;
      }
      .founded_by .inner_founded_by .title {
        max-width: 200px;
      }
    }

    @media screen and (min-width: 541px) and (max-width: 820px){
      {% comment %} .founded_by .inner_founded_by .title{
        max-width: unset;
      } {% endcomment %}
      .founded_by .inner_founded_by .right_content{
        margin-top: -58px;
        max-width: unset;
        border: 1px solid rgba(255,255,255,.3);
      }
      .founded_by .inner_founded_by .right_content p{
        font-size: 23px;
        font-weight: 600;
        line-height: 34px;
      }
      .founded_by .inner_founded_by .kevin_image_mobile{
        display: flex;
        justify-content: flex-end;
      }
      .founded_by .inner_founded_by .kevin_image_mobile img{
        display: block;
        margin-right: -20px;
      }
      .founded_by .inner_founded_by .title p{
        line-height: 73.98px;
      }
      .founded_by .inner_founded_by .right_content{
        margin-top: 50px;
      }
    }
  
    @media screen and (max-width: 540px) {
      .founded_by .inner_founded_by .title{
        max-width: 200px;
      }
      .founded_by .inner_founded_by .title p{
        font-size: 68px;
        line-height: 63.58px;
        letter-spacing: 1px;
      }
      .founded_by .inner_founded_by .right_content{
        margin-top: 82px;
        border: 1px solid rgba(255,255,255,.3);  
        position: relative;
        padding: 30px 20px 20px 20px;
      }
      .founded_by .inner_founded_by .right_content p{
        font-size: 18px;
        font-weight: 400;
        line-height: 21.6px;
      }
      .founded_by .inner_founded_by .right_content .content_quote{
        position: absolute;
        top: -25px;
      }
      .founded_by .inner_founded_by .kevin_image_mobile{
        width: 325px;
        height: 303px;
        position: absolute;
        top: 14px;
        right: -31px;
        display: block;
      }
      .founded_by .inner_founded_by .kevin_image_mobile img{
        width: 100%;
        height: 100%;
        object-fit: contain;
        object-position: center;
      }
      .founded_by .inner_founded_by .right_content{
        max-width: 100%;
      }
    }
    @media screen and (max-width: 360px) {
      .founded_by .inner_founded_by .kevin_image_mobile {
        width: 275px;
        height: 263px;
        position: absolute;
        top: 52px;
        right: -31px;
        display: block;
      }
    }
  </style>
  <div class="founded_by">
    <div class="inner_founded_by">
      {% if founded_by_title != blank %}
        <div class="title">
          <p>{{ founded_by_title }}</p>
        </div>
      {% endif %}
      <div class="kevin_image_desktop">
        <img src="{{ founder_desktop_image | img_url: '1440x' }}">
      </div>
      <div class="kevin_image_mobile">
        <img src="{{ founder_mobile_image | img_url: '820x' }} ">
      </div>
      <div class="right_content">
        <div class="content_quote">
          <svg width="43" height="40" viewBox="0 0 43 40" fill="none" xmlns="http://www.w3.org/2000/svg">
            <path d="M4.408 0.567997H15.622C14.02 5.552 12.418 12.138 12.24 16.944H19.894V39.728H0.136V21.394C0.136 12.494 2.806 4.30599 4.408 0.567997ZM27.192 0.567997H38.406C36.804 5.552 35.202 12.138 35.024 16.944H42.678V39.728H22.92V21.394C22.92 12.494 25.59 4.30599 27.192 0.567997Z" fill="#AFC42F"/>
          </svg>
        </div>
        {% if founded_by_subtitle != blank %}
          <p>{{ founded_by_subtitle }}</p>
        {% endif %}
        {% comment %} <p>At VitaHustle, we're on a mission to disrupt the game and make premium nutrition accessible to all. 
        Our commitment is simple: empower you to become the superhero version of yourself through simple, nutritious and damn delicious products.</p> {% endcomment %}
        
