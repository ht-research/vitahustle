  {% schema %}
    
  {
    "name": "Founded by",
    "settings": [
      {
        "type": "text",
        "id": "founded_by_title",
        "label": "t:sections.founded_by.founded_by_title"
      },
      {
          "type": "text",
          "id": "founded_by_subtitle",
          "label": "t:sections.founded_by.founded_by_subtitle"
      },
      {
        "type": "image_picker",
        "id": "founder_desktop_image",
        "label": "t:sections.founded_by.founder_desktop_image"
    },
    {
        "type": "image_picker",
        "id": "founder_mobile_image",
        "label": "t:sections.founded_by.founder_mobile_image"
    }
  
    ],
    "presets": [
      {
        "name": "Founded by"
      }
    ]
  }
  
  {% endschema %}